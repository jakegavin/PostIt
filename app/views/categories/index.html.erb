<%= render 'shared/content_title', title: 'All Categories' %>

<% @categories.each do |category| %>
  <div class="row">
    <div class="well">
      <h3><%= link_to category.name, category_path(category) %></h3>
      <p>
        <%= link_to "Edit", edit_category_path(category) %>
      </p>
    </div>
  </div>
<% end %>