<%= render 'shared/errors', obj: @category %>


<%= form_for @category do |f| %>
  <div class="input-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <br>
  <div class="input-group">
    <%= f.submit "Submit Category", class: "btn btn-primary" %>
  </div>
<% end %>